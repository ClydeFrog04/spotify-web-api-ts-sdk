{
  "name": "spotify-web-api-ts-skd",
  "version": "1.0.0",
  "description": "A typescript SDK for the Spotify Web API",
  "main": "index.js",
  "scripts": {
    "start": "npx vite serve",
    "test": "vitest run",
    "ci": "npm run test && npm run build",
    "build": "tsc"
  },
  "exports": {
    "import": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spotify/spotify-web-api-ts-sdk"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@peculiar/webcrypto": "^1.4.1",
    "@types/node": "^18.15.1",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "jsdom": "^21.1.1",
    "playwright": "^1.31.2",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "spotify",
    "webapi"
  ],
  "wallaby": {
    "autoDetect": [
      "vitest"
    ],
    "trace": true
  }
}
